{
  "manifest_version": 3,
  "name": "TaskMaster",
  "description": "Manage you tasks!",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "identity"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://localhost:8000/*"
  ]
}
